@model List<OnlineElectronicsStore.Domain.Entity.Product>

@foreach (var product in Model)
{
	for (int i = 0; i < 5; i++)
	{
		<section>
			<img src="">
			<p class="nameProduct">
				<a>@product.Name</a>
			</p>
			<div class="stars" data-rating="3"></div>
			<button class="wishList"><i class="fa-solid fa-heart"></i></button>
			<p class="priceProduct">@product.Price</p>
			<button class="cart"><i class="fa-solid fa-cart-shopping"></i></button>
		</section>
	}
}



